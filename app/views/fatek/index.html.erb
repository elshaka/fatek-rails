<div id="scada" data-uri="<%= request.host %>:<%= request.port %>/websocket" class="form-horizontal">
<!-- Form Name -->
<legend>FATEK Programmable Controller</legend>

<!-- Multiple Checkboxes (inline) -->
<div class="control-group">
  <label class="control-label" for="checkboxes">Salidas</label>
  <div class="controls">
    <label class="checkbox inline" for="checkboxes-0">
      <input type="checkbox" name="checkboxes" id="Y0" disabled="disabled">
      Y0
    </label>
    <label class="checkbox inline" for="checkboxes-1">
      <input type="checkbox" name="checkboxes" id="Y1" disabled="disabled">
      Y1
    </label>
    <label class="checkbox inline" for="checkboxes-2">
      <input type="checkbox" name="checkboxes" id="Y2" disabled="disabled">
      Y2
    </label>
    <label class="checkbox inline" for="checkboxes-3">
      <input type="checkbox" name="checkboxes" id="Y3" disabled="disabled">
      Y3
    </label>
  </div>
</div>

<div class="control-group">
  <label class="control-label" for="buttons">Controles</label>
  <div class="controls">
    <button id="run" name="buttons" class="btn btn-success">Run</button>
    <button id="stop" name="buttons" class="btn btn-danger">Stop</button>
  </div>
</div>
<div class="control-group">
  <label class="control-label" for="console">Consola</label>
  <pre id="console" name="console" class="console">Ping: -- ms</pre>
</div>
</div>